AddCommand("bulletgui", {"bulletgui", "cgui"}, "spawn bullet color GUI", function()
    if game.Players.LocalPlayer.Character.Head then
   local ScreenGui = Instance.new("ScreenGui")
local WhiteListframe = Instance.new("Frame")
local WhiteListTitle = Instance.new("TextLabel")
local Open = Instance.new("TextButton")
local TextBox = Instance.new("TextBox")
local ThemainGuiFrame = Instance.new("Frame")
local ClosePg2 = Instance.new("TextButton")
local Open2Frame = Instance.new("Frame")
local Open2 = Instance.new("TextButton")
--Properties:
ScreenGui.Parent = game.CoreGui
WhiteListframe.Name = "WhiteList frame"
WhiteListframe.Parent = ScreenGui
WhiteListframe.BackgroundColor3 = Color3.new(0, 0, 0)
WhiteListframe.BackgroundTransparency = 0.30000001192093
WhiteListframe.BorderColor3 = Color3.new(255, 0, 0)
WhiteListframe.BorderSizePixel = 5
WhiteListframe.Position = UDim2.new(0.649766397, 0, 0.544898021, 0)
WhiteListframe.Size = UDim2.new(0, 235, 0, 144)
WhiteListframe.Draggable = true
WhiteListframe.Selectable = true
WhiteListframe.Active = true

ScreenGui.Parent = game.CoreGui
WhiteListTitle.Name = "WhiteListTitle"
WhiteListTitle.Parent = WhiteListframe
WhiteListTitle.BackgroundColor3 = Color3.new(Black)
WhiteListTitle.BorderColor3 = Color3.new(255, 0, 0)
WhiteListTitle.Size = UDim2.new(0, 235, 0, 34)
WhiteListTitle.Font = Enum.Font.SourceSans
WhiteListTitle.Text = "Bullet Color"
WhiteListTitle.TextColor3 = Color3.new(255, 0, 0)
WhiteListTitle.TextSize = 30

Open.Name = "Open"
Open.Parent = WhiteListframe
Open.BackgroundColor3 = Color3.new(Black)
Open.Position = UDim2.new(0.0723404288, 0, 0.736111104, 0)
Open.Size = UDim2.new(0, 200, 0, 25)
Open.Font = Enum.Font.SourceSans
Open.Text = "Done"
Open.TextColor3 = Color3.new(255, 0, 0)
Open.TextSize = 20
Open.MouseButton1Down:connect(function()
	
	
	if TextBox.Text == 'red' then
	ThemainGuiFrame.Visible = true
	Open2Frame.Visible = true
        WhiteListframe.Visible = false
         game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Bullet Color";
    Text = "red";  
    Duration = 0.5 
	})
        local RunService = game:GetService("RunService")

RunService.RenderStepped:Connect(function()
    if game.Players.LocalPlayer.Character.Humanoid then
        
        if game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet") then
            
            if game.Players.LocalPlayer.Character.Humanoid.Bullet:findFirstChild("Trail") then
                
                if game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet").Name == "BulletDone" then
                    
                end
                if game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet"):findFirstChild("Trail").Lifetime < 0.21 then
                   
                    game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet").Trail.Lifetime = 0.21
                    game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet").Trail.Transparency = NumberSequence.new(0)
                    game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet").Trail.Color = ColorSequence.new(Color3.fromRGB(255,0,0),Color3.fromRGB(255, 0, 0))Color3.fromRGB(255,0,0)Color3.fromRGB(255, 0, 0)Color3.fromRGB(255,0,0)Color3.fromRGB(255, 0, 0)
                    game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet").Name = "BulletDone"
                end
            end
        end
    end
end)
        end

        if TextBox.Text == 'blue' then
	ThemainGuiFrame.Visible = true
	Open2Frame.Visible = true
        WhiteListframe.Visible = false
         game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Bullet Color";
    Text = "blue";  
    Duration = 0.5 
