local Owner = {
    [587419584] = true; 
}

getgenv().EspOwnerDude = function(OwnerDude)
    local bgui2 = INew("BillboardGui", OwnerDude.Character:WaitForChild("Head", 10))
    local tlab2 = INew("TextLabel", bgui2)
    bgui2.Adornee = OwnerDude.Character.Head
    bgui2.Size = UD2(0,100,0,100)
    bgui2.StudsOffset = Vec3(0,1,0)
    bgui2.AlwaysOnTop = true 
    tlab2.BackgroundTransparency = 1
    tlab2.Position = UD2(0,0,0,1)
    tlab2.Size = UD2(1,0,0,10)
    tlab2.TextStrokeTransparency = 0.1
    tlab2.Font = 'ArialBold'
    tlab2.TextSize = 20
    tlab2.TextColor3 = Color3.fromRGB(255,0,0);
    tlab2.Text = "0wner"
end

local LPRS = Players:GetPlayers()
for i = 1, #LPRS do
    local OwnerFun = LPRS[i]
    if Owner[OwnerFun.UserId] then
        local HeadFromIdiot = OwnerFun.Character:WaitForChild("Head", 10)
        if HeadFromIdiot then
            EspOwnerDude(OwnerFun)
        end
        OwnerFun.CharacterAdded:Connect(function()
            OwnerFun.Character:WaitForChild("Head", 10)
            EspOwnerDude(OwnerFun)
        end)
    end
end
                     
end
end)

