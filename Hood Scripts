		Sound.SoundId = active
		Sound.Volume = 2
		Sound:play()
		Play.Text = "Stop"
	else
		local Sound = Main.Sound
		Sound:remove()
		LogPlay = false
		Play.Text = "Play Audio"
	end
end)
