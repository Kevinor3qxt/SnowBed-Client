AddCommand("magicgun", {"magicshotty", "magingun"}, "Salven al Zumbzhizeru", function()
    if game.Players.LocalPlayer.Character.Head then
   
   game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("","All")
	game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Magin Shotty";
    Text = "Enabled!";
    Duration = 3,
	})


local getc = game.Players.LocalPlayer.Backpack.Shotty:GetChildren()
 
 
 game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "w" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(255,0,0)
    v.Transparency = 0.7
 
    end
end
 end
end)

 game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "a" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(255,255,0)
    v.Transparency = 0.7
 
    end
end
 end
end)

 game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "s" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(48, 255, 0)
    v.Transparency = 0.7
 
    end
end
 end
end)

game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "d" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(0, 255, 249)
    v.Transparency = 0.7
 
    end
end
 end
end)







    end

end)


AddCommand("vest detector", {"vestdetector", "vestusers"}, "thx icythugs", function()
    if game.Players.LocalPlayer.Character.Head then
   game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("","All")
	game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Vest detector";
    Text = "Press F9 to check all the vest users";
    Duration = 3,
	})
local Players = game:GetService'Players'
local Vest = {
    'BulletResist'
}

local function VestDetect(Player)
    local Character = Player.Character
    if Character then
        for _, v in next, Character:GetChildren() do
            if v:IsA'Folder' and v.Name == 'BulletResist' then
                print('This Player Has Vest ' .. Player.Name)
                return
            end
        end
        return warn(Player.Name .. ' This Player does not have a vest')
    else
        return 'Poop'
    end
end

Players.PlayerAdded:Connect(VestDetect)

for _, Player in next, Players:GetPlayers() do
    VestDetect(Player)
end




    end

end)


