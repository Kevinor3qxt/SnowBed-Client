AddCommand("kos group", {"huntgroup", "kos"}, "da da da daa", function(Args)
    if Args[1] then
if Args[1] == "leat" then
              Args[1] = "8016146"
end

if Args[1] == "woke" then
              Args[1] = "7371469"
end

if Args[1] == "sylenc" then
              Args[1] = "6131057"
end


  local function esp(part)
    local billgui = Instance.new('BillboardGui', part)
    local textlab = Instance.new('TextLabel', billgui)

    billgui.Name = "ESP"
    billgui.Adornee = part
    billgui.AlwaysOnTop = true
    billgui.ExtentsOffset = Vector3.new(0, 1, 0)
    billgui.Size = UDim2.new(0, 5, 0, 5)
    
    textlab.Name = 'kos Esp'
    textlab.BackgroundColor3 = Color3.new(255,0, 0)
    textlab.BackgroundTransparency = 1
    textlab.BorderSizePixel = 0
    textlab.Position = UDim2.new(0, 0, 0, -40)
    textlab.Size = UDim2.new(1, 0, 10, 0)
    textlab.Visible = true
    textlab.ZIndex = 10
    textlab.Font = 'ArialBold'
    textlab.FontSize = 'Size14'
    textlab.Text = "[KOS]"
    textlab.TextColor3 = Color3.fromRGB(255, 0, 0)
    textlab.TextStrokeColor3 = Color3.fromRGB(0,0,0)
    textlab.TextStrokeTransparency = 0.6
end

game.Players.PlayerAdded:Connect(function(plr)
    if plr:IsInGroup(Args[1]) then
        plr.CharacterAdded:Connect(function(char)
            repeat wait() until char:FindFirstChild("Humanoid")
            repeat wait() until char:FindFirstChild("Head")
            esp(char:FindFirstChild("Head"))
        end)
    end
end)

for i,plr in pairs(game.Players:GetPlayers()) do
    if plr:IsInGroup(Args[1]) then
        plr.CharacterAdded:Connect(function(char)
            repeat wait() until char:FindFirstChild("Humanoid")
            repeat wait() until char:FindFirstChild("Head")
            esp(char:FindFirstChild("Head"))
        end)
    end
end

    




end

end)



AddCommand("kick", {"kick", "left"}, "kick yourself with a nice message", function(Args)
    if Args then
 local KickMessage = table.concat(Args, " ")
 game:GetService("Players").LocalPlayer:Kick(KickMessage)
end
end)


AddCommand("abolute death", {"realdeath", "disintegration"}, "like thanos lol", function()
    if game.Players.LocalPlayer.Character.Head then


  game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Disintigration";
    Text = "On, they cant take u fake ss";
	})



  repeat wait() until game:IsLoaded()
        if game.PlaceId == 455366377 then
        while wait() do
        pcall(function()
        if game.Players.LocalPlayer.Character.Head:FindFirstChild("Bone") then
        game.Players.LocalPlayer.Character["Right Arm"]:Remove()
        game.Players.LocalPlayer.Character["Torso"]:Remove()
        game.Players.LocalPlayer.Character["Left Arm"]:Remove()
        game.Players.LocalPlayer.Character["Right Leg"]:Remove()
        game.Players.LocalPlayer.Character["Left Leg"]:Remove()
        game.Players.LocalPlayer.Character["Head"]:Remove()
        end
        end)
        end
        end
    end

end)

