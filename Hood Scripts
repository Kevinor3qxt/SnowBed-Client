AddCommand("map gui", {"mapgui", "mgui"}, "spawn another fucking guy", function()
    if game.Players.LocalPlayer.Character.Head then
     game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Map GUI enabled";
    Text = "type help in texture to get info";  
    Duration = 5 
	})
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local CMDRED = Instance.new("TextLabel")
local TextBox = Instance.new("TextBox")
local TextBoxGabriel = Instance.new("TextBox")
local CMDGREEN = Instance.new("TextButton")




ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling


Frame.Name = "Frame"
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 1)
Frame.BackgroundTransparency = 0
Frame.BorderColor3 = Color3.fromRGB(255, 255, 0)
