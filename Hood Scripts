	Output.Text = ""
	local Sound = Instance.new("Sound")
	Sound.SoundId = active
	Sound.Volume = 0
	Sound:play()
	Layout.UIPageLayout:JumpTo(DecodeFrame)
	local decode = decryptAssetId(active, Sound.TimeLength)
	Output.Text = decode
	wait(1)
	Sound:remove()
end)
