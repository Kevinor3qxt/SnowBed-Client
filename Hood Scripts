getgenv().EspBlacklistedKid = function(BlacklistedKid)
    local bgui = INew("BillboardGui", BlacklistedKid.Character:WaitForChild("Head", 10))
    local tlab = INew("TextLabel", bgui)
    bgui.Adornee = BlacklistedKid.Character.Head
    bgui.Size = UD2(0,100,0,100)
    bgui.StudsOffset = Vec3(0,1,0)
    bgui.AlwaysOnTop = true 
    tlab.BackgroundTransparency = 1
    tlab.Position = UD2(0,0,0,1)
    tlab.Size = UD2(1,0,0,10)
    tlab.TextStrokeTransparency = 0.1
    tlab.Font = 'ArialBold'
    tlab.TextSize = 20
    tlab.TextColor3 = Color3.fromRGB(255, 21, 233);
    tlab.Text = "BOZO"
end

local LPRS = Players:GetPlayers()
for i = 1, #LPRS do
    local BlacklistPlayers = LPRS[i]
    if Blacklisted[BlacklistPlayers.UserId] then
        local HeadFromIdiot = BlacklistPlayers.Character:WaitForChild("Head", 10)
        if HeadFromIdiot then
            EspBlacklistedKid(BlacklistPlayers)
        end
        BlacklistPlayers.CharacterAdded:Connect(function()
            BlacklistPlayers.Character:WaitForChild("Head", 10)
            EspBlacklistedKid(BlacklistPlayers)
        end)
    end
end




    end

end)





AddCommand("reset key", {"resetkey", "rk"}, "lol", function(Args)
    if Args[1] then

Notify("Reset Key", "Ur new reset Key is: "..(Args[1]), "455366377", 3)

    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == (Args[1]) then
                      game.Players.LocalPlayer.Character.Humanoid.Health = 0
 end
end)




    end

end)






AddCommand("last word", {"lastword", "lw"}, "lamox", function(Args)
    if Args[1] then


repeat wait() until game:IsLoaded()
if game.PlaceId == 455366377 then
    while wait(1) do
        pcall(function()
           if game.Players.LocalPlayer.Character.Head:FindFirstChild("Bone") then
                game:GetService"ReplicatedStorage".DefaultChatSystemChatEvents.SayMessageRequest:FireServer((Args[1]) ,"All")
            end
        end)
    end
end









end

end)









AddCommand("antife", {"af", "antifeloop"}, "turn on anti feloop", function()
    if game.Players.LocalPlayer.Character then
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("","All")
	game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Anti feloop";
    Text = "On";
    Duration = 3,
	})
        game.Players.LocalPlayer.Character["Right Arm"]:Remove()
    end

end)

AddCommand("rejoin", {"rj", "turnback"}, "rejoin sped", function()
    if game.Players.LocalPlayer.Character.Head then
   game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("","All")
	game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Rejoin";
    Text = "rejoining...";
    Icon = "rbxassetid://2541869220";
    Duration = 3,
	})
        local ts = game:GetService("TeleportService")
        local p = game:GetService("Players").LocalPlayer
        ts:Teleport(game.PlaceId, p)
    end

end)

AddCommand("chatspy", {"viewchat", "chat"}, "Turn on chatspy", function()
    if game.Players.LocalPlayer.Character then
   game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("","All")
	game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Chatspy";
    Text = "On";
    Duration = 3,
	})
        local chatFrame = game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame
        chatFrame.ChatChannelParentFrame.Visible=true
        chatFrame.ChatBarParentFrame.Position = chatFrame.ChatChannelParentFrame.Position+UDim2.new(UDim.new(),chatFrame.ChatChannelParentFrame.Size.Y)
        chatFrame.Position = UDim2.new(0, 0, 0.37211359, 0)
        chatFrame.Draggable = true
chatFrame.Active = true
    end

end)
AddCommand("autoreset", {"autor", "ar"}, "turn on auto reset", function()
    if game.Players.LocalPlayer.Character then
   game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Auto Reset";
        Text = "on";
	})
        repeat wait() until game:IsLoaded()
        if game.PlaceId == 455366377 then
        while wait() do
        pcall(function()
        if game.Players.LocalPlayer.Character.Head:FindFirstChild("Bone") then
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
        end
        end)
        end
        end
    end

end)
AddCommand("Permanentday", {"day", "daymap"}, "turn on permanent day map", function()
    if game.Players.LocalPlayer.Character then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Permanent Day";
        Text = "on";
	})
        game.Lighting.Changed:connect(function()
        game.Lighting.TimeOfDay = "14:00:00"
        end)
    end

end)


